@model IList<ItemModel>

<table class="table">
    <thead class="thead-light">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model[0].Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].IsCompleted)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].DueDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].CreatedDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IsCompleted)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DueDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CreatedDate)
                </td>
                <td>
                    <a asp-page="/Items/Edit" asp-route-id="@item.Id">Edit</a> |
                    <a asp-page="/Items/Details" asp-route-id="@item.Id">Details</a> |
                    <a asp-page="/Items/Delete" asp-route-id="@item.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>